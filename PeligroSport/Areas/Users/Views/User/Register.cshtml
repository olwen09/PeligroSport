    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script>



        $(document).ready(function () {

            //Create Account
            $("#userName").keydown(function () {
                console.log($("#userName").val());

                $("#userN").html($("#userName").val())
            });


            $("#firstName").keydown(function () {
                console.log($("#firstName").val());

                $("#firstN").html($("#firstName").val())
            });


            $("#lastName").keydown(function () {
                console.log($("#lastName").val());

                $("#lastN").html($("#lastName").val())
            });

            $("#phoneNumber").keydown(function () {
                console.log($("#phoneNumber").val());

                $("#phoneN").html($("#phoneNumber").val())
            });


            $("#emailContact").keydown(function () {
                console.log($("#emailContact").val());

                $("#email").html($("#emailContact").val())
            });
            //End Create Account

            //Address Details
            let addressZipCode;
            let addressCity;
            let addressState;
            let addressInfo;

            $("#addresLineOne").keydown(function () {
                console.log($("#addresLineOne").val());

                $("#addressOne").html($("#addresLineOne").val())
            });

            $("#addresLineTwo").keydown(function () {
                console.log($("#addresLineTwo").val());

                $("#addressTwo").html($("#addresLineTwo").val())
            });

            $("#addressZipCode").keydown(function () {

                addressZipCode = $("#addressZipCode").val();

                console.log(addressZipCode);

            });

            $("#addressCity").keydown(function () {

                addressCity = $("#addressCity").val();

            });

            $("#addressState").keydown(function () {

                addressState = $("#addressState").val();

            });

            $("#addressCountry").change(function () {

                addressInfo = addressCity + ", " + addressState + ", " + addressZipCode;

                $("#addressInfo").html(addressInfo);

            });
            //End Address Details


            //Address Shipping
            let shippingZipCode;
            let shippingCity;
            let shippingState;
            let shippingInfo;

            $("#shAddresLineOne").keydown(function () {

                $("#shippingOne").html($("#shAddresLineOne").val())

            });

            $("#shAddresLineTwo").keydown(function () {
                console.log($("#shAddresLineTwo").val());

                $("#shippingTwo").html($("#shAddresLineTwo").val())
            });

            $("#shZipCode").keydown(function () {

                shippingZipCode = $("#shZipCode").val();

                console.log(shippingZipCode);

            });

            $("#shCity").keydown(function () {

                shippingCity = $("#shCity").val();

            });

            $("#shState").keydown(function () {

                shippingState = $("#shState").val();

            });

            $("#shCountry").change(function () {

                shippingInfo = shippingCity + ", " + shippingState + ", " + shippingZipCode;

                $("#shippingInfo").html(shippingInfo);

            });
            //End Address Shipping



            //Billing Address
            let billingZipCode;
            let billingCity;
            let billingState;
            let billingInfo;

            $("#billingAddresLineOne").keydown(function () {

                $("#billingOne").html($("#billingAddresLineOne").val())

            });

            $("#billingAddresLineTwo").keydown(function () {
                //console.log($("#billingAddresLineTwo").val());

                $("#billingTwo").html($("#billingAddresLineTwo").val())
            });

            $("#billingAddresZipCode").keydown(function () {

                billingZipCode = $("#billingAddresZipCode").val();

            });

            $("#billingAddresCity").keydown(function () {

                billingCity = $("#billingAddresCity").val();

            });

            $("#billingAddresState").keydown(function () {

                billingState = $("#billingAddresState").val();

            });

            $("#billingAddresCountry").change(function () {

                billingInfo = billingCity + ", " + billingState + ", " + billingZipCode;

                $("#billingInfo").html(billingInfo);

            });
            //End Billing Address
        });

    </script>
    <!--end::Page Scripts-->


</body>
